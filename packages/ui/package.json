{
  "name": "ui",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist",
    "./styles.css": "./dist/styles.css"
  },
  "license": "MIT",
  "scripts": {
    "lint": "eslint src",
    "fix": "eslint src --fix",
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react && tailwindcss -i ./src/styles.css -o ./dist/styles.css",
    "dev": "concurrently \"tsup src/index.tsx --format esm,cjs --dts --external react --watch\" \"tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch\"",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@ladle/react": "^2.4.5",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "class-variance-authority": "^0.2.4",
    "concurrently": "^7.2.2",
    "eslint": "^8.26.0",
    "eslint-config-custom": "workspace:*",
    "react": "^18.2.0",
    "tailwind-config": "workspace:*",
    "tailwindcss": "^3.2.1",
    "tsconfig": "workspace:*",
    "tsup": "^6.1.3",
    "typescript": "^4.8.4"
  }
}
